{
    "compilerOptions": {
        "target": "ES5",
        "module": "CommonJS",
        "lib": ["ES5", "DOM"],
        "jsx": "react",
        // "noEmit": true,
        "sourceMap": true,

        /* Strict Type-Checking Options */
        "strict": true,
        "noImplicitAny": true,
        "strictNullChecks": true,

        /* Module Resolution Options */
        "moduleResolution": "node",
        "forceConsistentCasingInFileNames": true,
        "esModuleInterop": true
    },
    "include": ["src"]
}
